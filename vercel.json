{
    "version": 2,
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users/(.*)",
        "dest": "/routes/users.js"
      },
      {
        "src": "/api/mail/(.*)",
        "dest": "/routes/mail.js"
      },
      {
        "src": "/api/budget/(.*)",
        "dest": "/routes/budget.js"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  
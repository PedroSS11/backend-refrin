{
    "name": "refrin-back",
    "version": 2,
    "public": true,
    "build": {
        "src": "api/app.js",
        "use": "@vercel/node"
    },
    "routes": [
        {
            "src": "/api/users/(.*)",
            "dest": "api/routes.js"
        },
        {
            "src": "/api/mail/(.*)",
            "dest": "api/routes.js"
        },
        {
            "src": "/api/budget/(.*)",
            "dest": "api/routes.js"
        },
        {
            "src": "/(.*)",
            "dest": "api/app.js"
        }
    ]
}